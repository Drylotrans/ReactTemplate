{
    "name": "express-react-app",
    "version": "1.0.0",
    "description": "Simple React App with Express",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "build": "rimraf dist/ && webpack --mode production --config webpack.config.client.js && webpack --mode production --config webpack.config.server.js",
        "dev": "rimraf dist/ && concurrently \"webpack --mode development --config webpack.config.client.js --watch\" \"webpack --mode development --config webpack.config.server.js --watch\" \"nodemon dist/index.js -w dist/ -i dist/public/\"",
        "eslint": "eslint . --ext .jsx,.js",
        "eslint:fix": "eslint . --ext .jsx,.js --fix"
    },
    "keywords": [
        "template",
        "react",
        "express",
        "webpack",
        "node"
    ],
    "author": "Drylozu",
    "license": "MIT",
    "dependencies": {
        "express": "^4.17.1",
        "react": "^16.14.0",
        "react-dom": "^16.14.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.1",
        "@babel/preset-env": "^7.13.5",
        "@babel/preset-react": "^7.12.13",
        "babel-loader": "^8.2.2",
        "clean-webpack-plugin": "^3.0.0",
        "concurrently": "^6.0.0",
        "css-loader": "^5.1.0",
        "eslint": "^7.20.0",
        "eslint-plugin-react": "^7.22.0",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.2.0",
        "nodemon": "^2.0.7",
        "rimraf": "^2.7.1",
        "style-loader": "^2.0.0",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.0",
        "webpack-node-externals": "^2.5.2",
        "webpackbar": "^5.0.0-3"
    }
}
